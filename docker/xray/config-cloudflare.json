  "inbounds": [
    {
      "port": 10000,
      "listen": "0.0.0.0",
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "ab87d164-971c-44d8-8c34-b4e4ff9f2e71",
            "level": 0,
            "email": "admin@shaunstudio.vip"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
